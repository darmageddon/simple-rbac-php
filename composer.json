{
    "name": "darmageddon/simple-rbac-php",
    "type": "library",
    "description": "Simple RBAC Abstraction in PHP",
    "keywords": [
        "rbac"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Darmageddon\\RBAC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darmageddon\\RBAC\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Darma",
            "email": "fibodarma@gmail.com"
        }
    ],
    "require": {
        "php": "~8.0.0|~8.1.0|~8.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
